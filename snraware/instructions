
INSTRUCTIONS

MRD version: 
pip install mrd-python@git+https://github.com/ismrmrd/mrd.git@0f7684d55cc80d9e69cbe4df21e6a4f3b41515e3#subdirectory=python

python3 fromMATtoMRD3D_RARE_noise_v2.py --input phantom_noise.mat --output phantom_noise_2.mrd

data_id=$(tyger buffer create)

cat phantom_noise.mrd  | tyger buffer write $data_id

tyger buffer show $data_id

noisecovariance_id=$(tyger buffer create)

run_id=$(tyger run create -f noise_adjustment.yml --buffer noisecovariance=$noisecovariance_id --buffer input=$data_id)

tyger run show $run_id

recon_id=$(tyger buffer create --tag scan_type=3D --tag recon=ai)

tyger buffer show $recon_id

run_id=$(tyger run create -f run.yml --buffer noisecovariance=$noisecovariance_id --buffer input=$data_id --buffer recon=$recon_id)

tyger run show $run_id

tyger buffer read $recon_id >phantom_ia_2.mrd

tyger run logs $run_id

python3 read.py


##### 

python3 fromMATtoMRD3D_RARE_noise_v2.py --input phantom_noise.mat --output phantom_noise_3.mrd

data_id=$(tyger buffer create)

cat phantom_noise.mrd  | tyger buffer write $data_id

noisecovariance_id=$(tyger buffer create)

run_id=$(tyger run create -f noise_adjustment.yml --buffer noisecovariance=$noisecovariance_id --buffer input=$data_id)

recon_id=$(tyger buffer create --tag scan_type=3D --tag recon=ai)

run_id=$(tyger run create -f run.yml --buffer noisecovariance=$noisecovariance_id --buffer input=$data_id --buffer recon=$recon_id)

tyger buffer read $recon_id >phantom_ia_2.mrd

python3 read.py

